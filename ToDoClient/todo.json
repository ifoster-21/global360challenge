{
  "openapi": "3.1.1",
  "info": {
    "title": "Ianf.Global360ToDo.WebAPI | v1",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:5124/"
    }
  ],
  "paths": {
    "/ToDoItems": {
      "get": {
        "tags": [
          "ToDo"
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/ToDo"
                  }
                }
              },
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/ToDo"
                  }
                }
              },
              "text/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/ToDo"
                  }
                }
              }
            }
          }
        }
      },
      "post": {
        "tags": [
          "ToDo"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/NewToDo"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/NewToDo"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/NewToDo"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/ToDoId"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ToDoId"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/ToDoId"
                }
              }
            }
          }
        }
      }
    },
    "/ToDoItems/{toDoId}": {
      "delete": {
        "tags": [
          "ToDo"
        ],
        "parameters": [
          {
            "name": "toDoId",
            "in": "path",
            "required": true,
            "schema": {
              "pattern": "^-?(?:0|[1-9]\\d*)$",
              "type": [
                "integer",
                "string"
              ],
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "NewToDo": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string"
          },
          "contents": {
            "type": "string"
          },
          "priority": {
            "$ref": "#/components/schemas/Priority"
          },
          "completionDate": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "Priority": {
        "type": "integer"
      },
      "ToDo": {
        "type": "object",
        "properties": {
          "id": {
            "$ref": "#/components/schemas/ToDoId"
          },
          "title": {
            "type": "string"
          },
          "contents": {
            "type": "string"
          },
          "priority": {
            "$ref": "#/components/schemas/Priority"
          },
          "completionDate": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "ToDoId": {
        "type": "object",
        "properties": {
          "id": {
            "pattern": "^-?(?:0|[1-9]\\d*)$",
            "type": [
              "integer",
              "string"
            ],
            "format": "int32"
          }
        }
      }
    }
  },
  "tags": [
    {
      "name": "ToDo"
    }
  ]
}